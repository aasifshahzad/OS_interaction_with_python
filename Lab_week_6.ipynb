    "# findJane.sh\n",
    "#!/bin/bash\n",
    "grep \" jane \" ../data/list.txt | cut -d ' ' -f 3 > tempFile.txt\n",
    "files=$(<tempFile.txt)\n",
    "for i in $files ; do\n",
    " if test -e \"..$i\"; then echo \"$i\" >> oldFiles.txt;\n",
    " fi\n",
    "done"
   ]
